{
  "PORT": 3240,
  "PERIODIC_INTERVAL": 1200,

  "GIT_USERNAME": "Hook Machine",
  "GIT_USEREMAIL": "hook.machine@example.com",

  "KEY_MAIN": "",
  "HOOK_SECRET": "",

  "POST_RECEIVE_HOOKS": {},
  "PERIODIC_HOOKS": {},

  "WOODMAN": {
    "appenders": [
      {
        "type": "ConsoleAppender",
        "name": "console",
        "layout": {
          "type": "pattern",
          "pattern": "%-5level | %-15.15logger | %message%n",
          "compactObjects": true
        }
      }
    ],
    "loggers": [
      {
        "level": "all",
        "appenders": [
          "console"
        ]
      }
    ]
  }
}